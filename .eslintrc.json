{
    "extends": "airbnb",
    "env" : {
        "browser": true,
        "jest" : true
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension" : "off",
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "all",
            "printWidth" : 100
        }]
    }
}